{
  "name": "n8n-nodes-qlynk",
  "version": "1.1.2",
  "description": "n8n nodes for Qlynk URL shortener - Create, manage and track short links as workflow nodes and AI agent tools",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "qlynk",
    "url-shortener",
    "link-management",
    "ai-tool",
    "ai-agent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ClosiQode/n8n-nodes-qlynk",
  "author": {
    "name": "ClosiQode",
    "email": "contact@qlynk.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClosiQode/n8n-nodes-qlynk.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/QlynkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Qlynk/Qlynk.node.js",
      "dist/nodes/QlynkUrlCreate/QlynkUrlCreate.node.js",
      "dist/nodes/QlynkUrlGet/QlynkUrlGet.node.js",
      "dist/nodes/QlynkUrlList/QlynkUrlList.node.js",
      "dist/nodes/QlynkUrlUpdate/QlynkUrlUpdate.node.js",
      "dist/nodes/QlynkUrlDelete/QlynkUrlDelete.node.js",
      "dist/nodes/QlynkCategoryCreate/QlynkCategoryCreate.node.js",
      "dist/nodes/QlynkCategoryGet/QlynkCategoryGet.node.js",
      "dist/nodes/QlynkCategoryList/QlynkCategoryList.node.js",
      "dist/nodes/QlynkCategoryUpdate/QlynkCategoryUpdate.node.js",
      "dist/nodes/QlynkCategoryDelete/QlynkCategoryDelete.node.js",
      "dist/nodes/QlynkStats/QlynkStats.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.52.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
